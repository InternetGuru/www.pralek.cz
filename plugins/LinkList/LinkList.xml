<?xml version="1.0" encoding="utf-8"?>
<LinkList>
  <var id="template">long</var>
  <item id="long" modifyonly="modifyonly">
    <dl class="clanek" var="linklist-new@class">
      <dt><a title="Zobrazit článek %s" var="link@href short@title" href="%s?usp=image"><img var="image@src heading@alt"/></a></dt>
      <dd class="date" fn="inputvar-dateformat" var="ctime">n/a</dd>
      <dd class="kw" var="kw" fn="inputvar-maketags">n/a</dd>
      <dt class="heading"><a title="Zobrazit článek %s" var="link@href short@title heading" href="%s?usp=heading">n/a</a></dt>
      <dd class="desc" var="desc">n/a</dd>
      <dd class="extra"><ul>
        <li><a title="Zobrazit článek %s" var="link@href short@title" class="button" href="%s?usp=button">Přečíst</a></li>
        <li>
          <span class="hideable">
            <span var="link@data-eventable-label" class="eventable" data-eventable-action="share"><span var="inputvar-link-ico"/>share</span>
            <span class="comment hideable-nohide"><a var="fbcommentid@href inputvar-fbcomment-title@title" href="https://www.facebook.com/pralek/posts/%s"><span var="inputvar-fbcomment-ico"/>Diskutovat na našem Facebooku</a></span>
            <span class="copyable" fn="inputvar-createlink" var="link">n/a</span>
            <span class="fb hideable-nohide"><a var="link@href inputvar-fb-title@title" href="https://www.facebook.com/sharer/sharer.php?u=https://www.pralek.cz/%s"><span var="inputvar-facebook-ico"/>Sdílet na facebooku</a></span>
            <span class="twitter hideable-nohide"><a var="link@href inputvar-twitter-title@title" href="https://twitter.com/intent/tweet?text=www.pralek.cz/%s"><span var="inputvar-twitter-ico"/>Sdílet na twitteru</a></span>
          </span>
        </li>
      </ul></dd>
    </dl>
  </item>
</LinkList>
